<button class="toggle-sidebar">
  <svg width="30px" height="30px" viewBox="0 0 30 30">
    <g fill="#737373">
      <rect class="rect-1" x="5" y="8" width="20" height="2"></rect>
      <rect class="rect-2" x="5" y="14" width="20" height="2"></rect>
      <rect class="rect-3" x="5" y="20" width="20" height="2"></rect>
    </g>
  </svg>
  <span class="text">Navigation</span>
</button>

<p class="ui large breadcrumb">
  {# TODO: render parents #}

  {% if  united_structure_item().route == united_structure_item().subRoute %}
    <span class="active section" >{{ united_structure_item()|trans }}</span>
  {% else %}
    <a class="section" href="{{ path(united_structure_item().route) }}">{{ united_structure_item()|trans }}</a>
    <span class="divider"> / </span>
    <span class="active section" >{{ united_structure_item().subRoute|trans }}</span>
  {% endif %}
</p>