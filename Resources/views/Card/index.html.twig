{% extends united_theme('layout.html.twig') %}
{% block content %}

  <div class="ui four stackable doubling cards">
    <a href="create" class="single button card">
      <div class="container">
        <div class="content">
          <div class="center">
            <div class="ui positive small labeled icon button"><i class="icon add"></i> {% trans %}Add{% endtrans %}</div>
          </div>
        </div>
      </div>
    </a>
    {% for entity in entities %}
      {% include cardTemplate with {'entity': entity} %}
    {% endfor %}
  </div>

{% endblock %}